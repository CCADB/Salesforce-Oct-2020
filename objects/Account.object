IF(AND ((Today() - (Standard_Audit_Statement_Date__c + 365) &gt;= 0),(Today() - (Standard_Audit_Statement_Date__c + 365) &lt;= 90)), &apos;2. Audit Alert : Standard Audit Date &lt; 90 days past due&apos;,

/* Check for &gt; 90 past due past due Standard Audit Date */ 
IF((TODAY() - (Standard_Audit_Statement_Date__c + 365) &gt; 90), &apos;3. Audit Alert : Standard Audit Date &gt; 90 days past due&apos;,

/* Std Audit Date ok; Check for NULL BR Audit Date */ 
IF(ISBLANK(BR_Audit_Statement_Date__c), &apos;4. Audit Alert : Standard Audit Date ok but BR Audit Date Null&apos;,

