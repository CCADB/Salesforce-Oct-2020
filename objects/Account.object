        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Audit_Date_Alert_For_Intermediate_Cert__c</fullName>
        <description>For Intermediate certs check for audits and audit dates
- if std audit blank then flag error
- else if std stmt dt null or old then flag error
- else if br audit not blank &amp; br stmt dt null or old then flag error
- else if ev ssl audit not blank &amp; ev ssl stmt dt null or old then flag error
- else if ev code signing audit not blank &amp; ev code signing stmt dt null or old then flag error</description>
        <externalId>false</externalId>
        <formula>IF (AND (RecordType.Name = &apos;Intermediate Certificate&apos;,  Audits_Same_as_Parent__c = False), 

/* Check if Standard Audit Missing and Standard Audit Date Expired */
IF(ISBLANK(Standard_Audit__c),IMAGE (&quot;/resource/Exclamation&quot;, &quot;Exclamation&quot;) &amp; &quot;StdAuditNull&quot;,
IF (ISBLANK (Standard_Audit_Statement_Date__c), IMAGE (&quot;/resource/Flag_Red&quot;, &quot;Flag Red&quot;) &amp; &quot;StdAuditDtNull&quot;, 
IF (Today() - Standard_Audit_Statement_Date__c &gt; 365,IMAGE (&quot;/resource/Flag_Red&quot;, &quot;Flag Red&quot;) &amp; &quot;StdAuditDt&gt;365&quot;, &quot;&quot;))) 
&amp; 
/* And check if BR Audit Date blank or old */
IF(NOT(ISBLANK(BR_Audit__c)), 
   IF(ISBLANK(BR_Audit_Statement_Date__c),
      IMAGE (&quot;/resource/Flag_Red&quot;, &quot;Flag Red&quot;) &amp; &quot;BRAuditDtNull&quot;,
         IF (Today() - BR_Audit_Statement_Date__c &gt; 365, 
             IMAGE (&quot;/resource/Flag_Red&quot;, &quot;Flag Red&quot;) &amp; &quot;BRAuditDt&gt;365&quot;,&quot;&quot;)),&quot;&quot;) 
&amp;
/* And check if EV SSL Audit Date blank */
IF(NOT(ISBLANK(EV_Audit__c)), 
   IF(ISBLANK(EV_Audit_Statement_Date__c),
      IMAGE (&quot;/resource/Flag_Red&quot;, &quot;Flag Red&quot;) &amp; &quot;EVSSLAuditDtNull&quot;,
         IF (Today() - EV_Audit_Statement_Date__c &gt; 365, 
             IMAGE (&quot;/resource/Flag_Red&quot;, &quot;Flag Red&quot;) &amp; &quot;EVSSLAuditDt&gt;365&quot;,&quot;&quot;)),&quot;&quot;) 
&amp;
/* And check if EV Code Signing Audit Date blank - THIS CHECK IS NOT NEEDED BY MOZILLA */
IF(NOT(ISBLANK(EV_Code_Signing_Audit__c)), 
   IF(ISBLANK(EV_Code_Signing_Audit_Statement_Date__c),
      IMAGE (&quot;/resource/Flag_Red&quot;, &quot;Flag Red&quot;) &amp; &quot;EVCodeSigningAuditDtNull&quot;,
         IF (Today() - EV_Code_Signing_Audit_Statement_Date__c &gt; 365, 
             IMAGE (&quot;/resource/Flag_Red&quot;, &quot;Flag Red&quot;) &amp; &quot;EVCodeSigningAuditDt&gt;365&quot;,&quot;&quot;)),&quot;&quot;) 
,
&quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>For Intermediate Certs, show error messages for missing audit documents and missing/expired dates</inlineHelpText>
        <label>Audit Date Alert For Intermediate Cert</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Audit_Date_Alert__c</fullName>
        <description>No flag- Standard Audit Statement Date is newer than 1 year ago 
Yellow Flag - Standard Audit Statement Date is older than 12 months, but not more than 18 months old
Red Flag - Standard Audit Statement Date is older than 18 months old, but not more than 2 years old
Exclamation - Standard Audit Statement Date is older than 2 years</description>
        <externalId>false</externalId>
        <formula>IF (TODAY() - Standard_Audit_Statement_Date__c &gt; 730, 
   IMAGE(&quot;/resource/Exclamation&quot;, &quot;Exclamation&quot;), 
   IF ( TODAY() - Standard_Audit_Statement_Date__c &gt; 547,
      IMAGE(&quot;/resource/Flag_Red&quot;, &quot;Red flag&quot;), 
      IF ( TODAY() - Standard_Audit_Statement_Date__c &gt; 365,
         IMAGE(&quot;/resource/Flag_Yellow&quot;, &quot;Yellow flag&quot;), &quot; &quot;
      )
   )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Audit Date Alert</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
