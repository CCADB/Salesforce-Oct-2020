        <description>if  &apos;CP/CPS Sames as Parent&apos; field is checked, all the related fields should be blank</description>
        <errorConditionFormula>AND(CP_CPS_Same_as_Parent__c  = TRUE, 
     OR( Policy_Documentation__c  &lt;&gt; NULL,
         CA_Document_Repository__c  &lt;&gt; NULL, 
         Certificate_Policy_CP__c  &lt;&gt; NULL,
         Certification_Practice_Statement_CPS__c  &lt;&gt; NULL
       )
    )</errorConditionFormula>
        <errorDisplayField>CP_CPS_Same_as_Parent__c</errorDisplayField>
        <errorMessage>&apos;CP/CPS Same as Parent&apos; is checked. Please uncheck this field or clear rest of the Policies and Practices Information fields to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Date_of_Revocation_not_blank</fullName>
        <active>true</active>
        <description>Check Date of Revocation not blabk when Revocation Status = &apos;Revoked&apos;</description>
        <errorConditionFormula>AND ($Profile.Name &lt;&gt; &apos;System Administrator - Mozilla&apos;, ISPICKVAL(Revocation_Status__c, &quot;Revoked&quot;), ISBLANK(Date_of_Revocation__c))</errorConditionFormula>
        <errorDisplayField>Date_of_Revocation__c</errorDisplayField>
        <errorMessage>Please fill in the date when this intermediate certificate was revoked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Status_cannot_be_Verified_by_CA</fullName>
        <active>true</active>
        <description>A community user cannot choose &apos;Verified&apos; status</description>
        <errorConditionFormula>AND ( $Profile.Name = &apos;Customer Community Plus User - CAs&apos;, 
      ISPICKVAL (Status__c, &apos;Verified&apos;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>&apos;Verified&apos; status cannot be chosen by a CA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_SHA_1_Format</fullName>
        <active>false</active>
        <description>DO NOT USE
Check for SHA-1 Format</description>
        <errorConditionFormula>AND (OR(ISPICKVAL(Type,&apos;Root&apos;), ISPICKVAL(Type,&apos;Intermediate Certificate&apos;)),
  NOT (OR (LEN(SHA_1_Fingerprint__c) = 0, 
            REGEX(SHA_1_Fingerprint__c, &quot;[\\w]{40}&quot;),
            REGEX(SHA_1_Fingerprint__c, &quot;[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}&quot;)
      )
   )
)</errorConditionFormula>
        <errorDisplayField>SHA_1_Fingerprint__c</errorDisplayField>
        <errorMessage>DO NOT USE Please enter SHA-1 Fingerprint as a string of 40 characters or string of 59 characters with &apos;:&apos; between every two characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_SHA_256_Format</fullName>
        <active>false</active>
        <description>DO NOT USE
Check for SHA-256 Format</description>
        <errorConditionFormula>AND (OR(ISPICKVAL(Type,&apos;Root&apos;), ISPICKVAL(Type,&apos;Intermediate Certificate&apos;)), 
NOT (OR (LEN(SHA_256_Fingerprint__c) = 0, 
REGEX(SHA_256_Fingerprint__c, &quot;[\\w]{64}&quot;), 
REGEX(SHA_256_Fingerprint__c, &quot;[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}:[\\w]{2}&quot;) 
) 
) 
)</errorConditionFormula>
        <errorDisplayField>SHA_256_Fingerprint__c</errorDisplayField>
        <errorMessage>DO NOT USE Please enter SHA-256 Fingerprint as a string of 64 characters or string of 95 characters with &apos;:&apos; between every two characters</errorMessage>
    </validationRules>
