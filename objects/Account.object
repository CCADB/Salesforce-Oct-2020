    <fields>
        <fullName>Date_of_Revocation_GMT__c</fullName>
        <description>This formula field converts regular date field to display date in YYYY MMM DD format. Eg: &quot;7/9/1999&quot; shows as &quot;1999 Jul 09&quot;</description>
        <externalId>false</externalId>
        <formula>IF (NOT ISBLANK(Date_of_Revocation__c), 
TEXT(YEAR(Date_of_Revocation__c)) &amp; &quot; &quot; &amp; 
CASE(MONTH(Date_of_Revocation__c), 
1, &quot;Jan&quot;, 
2, &quot;Feb&quot;, 
3, &quot;Mar&quot;, 
4, &quot;Apr&quot;, 
5, &quot;May&quot;, 
6, &quot;Jun&quot;, 
7, &quot;Jul&quot;, 
8, &quot;Aug&quot;, 
9, &quot;Sep&quot;, 
10, &quot;Oct&quot;, 
11, &quot;Nov&quot;, 
12, &quot;Dec&quot;, 
NULL) &amp; &quot; &quot; &amp; 
IF (LEN(TEXT(DAY(Date_of_Revocation__c))) = 1,&quot;0&quot; &amp; TEXT(DAY(Date_of_Revocation__c)),TEXT(DAY(Date_of_Revocation__c))), 
NULL)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Date of Revocation [GMT]</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
