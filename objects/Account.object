        <fullName>CheckBRAudPeriodEndDtLessThanEndDate</fullName>
        <active>true</active>
        <description>End Date should be greater than or equal to Start Date</description>
        <errorConditionFormula>AND (OR(RecordType.Name = &quot;Root Certificate&quot;, RecordType.Name = &quot;Intermediate Certificate&quot;), (BR_Audit_Period_End_Date__c &lt; BR_Audit_Period_Start_Date__c))</errorConditionFormula>
        <errorDisplayField>BR_Audit_Period_End_Date__c</errorDisplayField>
        <errorMessage>BR Audit Period End Date should be greater than or equal to BR Audit Period Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckBRAudPeriodEndDtNotLaterThanStDate</fullName>
        <active>true</active>
        <description>Audit Period End Date cannot be later than the Audit Statement Date</description>
        <errorConditionFormula>AND (OR(RecordType.Name = &quot;Root Certificate&quot;, RecordType.Name = &quot;Intermediate Certificate&quot;), (BR_Audit_Period_End_Date__c &gt; BR_Audit_Statement_Date__c))</errorConditionFormula>
        <errorDisplayField>BR_Audit_Period_End_Date__c</errorDisplayField>
        <errorMessage>BR Audit Period End Date cannot be later than the BR Audit Statement Date</errorMessage>
