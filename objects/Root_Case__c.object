    <validationRules>
        <fullName>Validate_Test_Website_Expired_URL</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( 
OR(LEN (Test_Website_Expired__c) = 0, 
REGEX(Test_Website_Expired__c, &quot;^[hH][tT][tT][pP][sS][0-9A-Za-z+&amp;@#/%?=~\\-_|!:,.;\\[\\]()]*$&quot;) 
) 
)</errorConditionFormula>
        <errorDisplayField>Test_Website_Expired__c</errorDisplayField>
        <errorMessage>Please provide valid test website link, URL must begin with &apos;https&apos; and can only contain letters, numbers and following characters: + &amp; @ # / % ? = ~- _ | ! : , . ; [ ] ( )</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Test_Website_Revoked_URL</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( 
OR(LEN (Test_Website_Revoked__c) = 0, 
REGEX(Test_Website_Revoked__c, &quot;^[hH][tT][tT][pP][sS][0-9A-Za-z+&amp;@#/%?=~\\-_|!:,.;\\[\\]()]*$&quot;) 
) 
)</errorConditionFormula>
        <errorDisplayField>Test_Website_Revoked__c</errorDisplayField>
        <errorMessage>Please provide valid test website link, URL must begin with &apos;https&apos; and can only contain letters, numbers and following characters: + &amp; @ # / % ? = ~- _ | ! : , . ; [ ] ( )</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Test_Website_Valid_URL</fullName>
        <active>true</active>
