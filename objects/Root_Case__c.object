OR(LEN (Test_Website_Valid__c) = 0, 
REGEX(Test_Website_Valid__c, &quot;^[hH][tT][tT][pP][sS][0-9A-Za-z+&amp;@#/%?=~\\-_|!:,.;\\[\\]()]*$&quot;) 
) 
)</errorConditionFormula>
