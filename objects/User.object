    <sharingModel>Read</sharingModel>
    <validationRules>
        <fullName>DoNotAllowCAsToChangeProfileFields</fullName>
        <active>true</active>
        <description>Do not allow external users to update the profile fields</description>
        <errorConditionFormula>AND( 
OR(ISCHANGED(FirstName), 
   ISCHANGED(LastName), 
   ISCHANGED(Email)), 
TEXT($User.UserType) != &quot;Standard&quot;)</errorConditionFormula>
        <errorMessage>Please contact support@ccadb.org to make any changes to profile.</errorMessage>
    </validationRules>
