    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <enableFeeds>true</enableFeeds>
    <externalSharingModel>Private</externalSharingModel>
    <fields>
        <fullName>Additional_User_Record__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Check this box if there are more than one user records for a user with using same email address. Custom Mass Email batch program uses this field to send emails</inlineHelpText>
        <label>Additional User Record</label>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CA_Owner_for_Contact__c</fullName>
        <externalId>false</externalId>
        <formula>Contact.Account.Name</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>CA Owner for Contact</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CA_Program_Manager__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Mozilla CA Program Manager</label>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_User_Email_Same_As_Contact_Email__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Email = Contact.Email, true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is User Email Same As Contact Email?</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_User_Name_Same_As_Contact_Name__c</fullName>
        <description>IF(AND(FirstName = Contact.FirstName, LastName = Contact.LastName), true, false)</description>
        <externalId>false</externalId>
        <formula>IF(AND(FirstName = SUBSTITUTE(Contact.FirstName, &apos;**&apos;, &apos;&apos;), LastName = Contact.LastName), true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is User Name Same As Contact Name?</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ManagerId</fullName>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Hierarchy</type>
    </fields>
    <fields>
        <fullName>Microsoft_CA_Program_Manager__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Microsoft CA Program Manager</label>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>User_Id_18_digit__c</fullName>
        <externalId>false</externalId>
        <formula>CASESAFEID(Id)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>User Id (18 digit)</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <listViews>
