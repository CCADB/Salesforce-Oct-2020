    <validationRules>
        <fullName>CheckCommunityUserUpdate</fullName>
        <active>true</active>
        <description>Do not allow users to edit key fields if the CA Contact record is also a community user</description>
        <errorConditionFormula>AND (RecordType.Name = &apos;CA Contact&apos;, 
    Is_Community_User__c = TRUE, 
    TEXT(Type__c) != &apos;Obsolete&apos;, 
     OR (ISCHANGED(FirstName), 
         ISCHANGED(LastName), 
         ISCHANGED(Email)) 
)</errorConditionFormula>
        <errorMessage>This CA is also a community user. To make changes to Name or Email fields, please contact your System Administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckIfHasBugzillaAcountIsChecked</fullName>
        <active>true</active>
        <description>&apos;Has Bugzilla Account?&apos; must be checked when Bugzilla Username is not blank</description>
        <errorConditionFormula>AND (Has_Bugzilla_Account__c = False, NOT(ISBLANK(Bugzilla_Username__c)))</errorConditionFormula>
        <errorDisplayField>Has_Bugzilla_Account__c</errorDisplayField>
        <errorMessage>&apos;Has Bugzilla Account?&apos; must be checked when Bugzilla Username has data</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateCAOwnerForContacts</fullName>
        <active>true</active>
        <description>If the Contact Record Type is &quot;CA Contact&quot; then CA Owner/Certificate Name should be of type &quot;CA Owner&quot;</description>
        <errorConditionFormula>AND(RecordType.Name = &quot;CA Contact&quot;,  Account.RecordType.Name &lt;&gt; &quot;CA Owner&quot;)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Please select CA Owner/Certificate Name of type &quot;CA Owner&quot;</errorMessage>
    </validationRules>
