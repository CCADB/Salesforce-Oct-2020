    <validationRules>
        <fullName>Validate_Auditor_Website</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( 
OR(LEN (Auditor_Website__c) = 0, 
REGEX(Auditor_Website__c, &quot;^[hH][tT][tT][pP][0-9A-Za-z+&amp;@#/%?=~\\-_|!:,.;\\[\\]()]*$&quot;) 
) 
)</errorConditionFormula>
        <errorDisplayField>Auditor_Website__c</errorDisplayField>
        <errorMessage>Auditor Website URL must begin with &apos;http&apos; and can only contain letters, numbers and following characters: + &amp; @ # / % ? = ~ -_ | ! : , . ; [ ] ( )</errorMessage>
    </validationRules>
