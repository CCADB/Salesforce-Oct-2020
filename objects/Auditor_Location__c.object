    <validationRules>
        <fullName>Validate_Auditor_Qualifications</fullName>
        <active>false</active>
        <errorConditionFormula>NOT( 
OR(LEN (Auditor_Qualifications__c) = 0, 
REGEX(Auditor_Qualifications__c, &quot;^[hH][tT][tT][pP][0-9A-Za-z+&amp;@#/%?=~\\-_|!:,.;\\[\\]()]*$&quot;) 
) 
)</errorConditionFormula>
        <errorDisplayField>Auditor_Qualifications__c</errorDisplayField>
        <errorMessage>Auditor Qualifications URL must begin with &apos;http&apos; and can only contain letters, numbers and following characters: + &amp; @ # / % ? = ~ -_ | ! : , . ; [ ] ( )</errorMessage>
    </validationRules>
