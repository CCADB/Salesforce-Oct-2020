    <validationRules>
        <fullName>Check_for_From_User</fullName>
        <active>true</active>
        <errorConditionFormula>IF (OR (ISPICKVAL(From__c, &quot;Current User&quot;), ISPICKVAL(From__c, &quot;CCADB Support&quot;)), FALSE,
AND(
NOT(AND (CONTAINS($Profile.Name, &quot;Apple&quot;),
 ISPICKVAL(From__c,&quot;Apple CA Program Manager&quot;))),
NOT(AND (CONTAINS($Profile.Name, &quot;Cisco&quot;),
 ISPICKVAL(From__c,&quot;Cisco CA Program Manager&quot;))),
NOT(AND (CONTAINS($Profile.Name, &quot;Mozilla&quot;),
 ISPICKVAL(From__c,&quot;Mozilla CA Program Manager&quot;))),
NOT(AND (CONTAINS($Profile.Name, &quot;Microsoft&quot;),
 ISPICKVAL(From__c,&quot;Microsoft CA Program Manager&quot;))),
NOT(AND (CONTAINS($Profile.Name, &quot;Google&quot;),
 ISPICKVAL(From__c,&quot;Google CA Program Manager&quot;)))
)
)</errorConditionFormula>
        <errorMessage>Please select &apos;From&apos; address which represent your company or select &apos;CCADB Support&apos; &apos;Current User&apos; from the list</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Reply_To_User</fullName>
        <active>true</active>
        <errorConditionFormula>IF (ISPICKVAL(Reply_To__c, &quot;Current User&quot;), FALSE,
AND(
NOT(AND (CONTAINS($Profile.Name, &quot;Apple&quot;),
 ISPICKVAL(Reply_To__c,&quot;Reply-To Apple CA Program Manager&quot;))),
NOT(AND (CONTAINS($Profile.Name, &quot;Cisco&quot;),
 ISPICKVAL(Reply_To__c,&quot;Reply-To Cisco CA Program Manager&quot;))),
NOT(AND (CONTAINS($Profile.Name, &quot;Mozilla&quot;),
 ISPICKVAL(Reply_To__c,&quot;Reply-To Mozilla CA Program Manager&quot;))),
NOT(AND (CONTAINS($Profile.Name, &quot;Microsoft&quot;),
 ISPICKVAL(Reply_To__c,&quot;Reply-To Microsoft CA Program Manager&quot;))),
NOT(AND (CONTAINS($Profile.Name, &quot;Google&quot;),
 ISPICKVAL(Reply_To__c,&quot;Reply-To Google CA Program Manager&quot;)))
)
)</errorConditionFormula>
        <errorMessage>Please select &apos;Reply To&apos; address which represent your company or select &apos;Current User&apos; from the list</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_Mass_Email</fullName>
        <active>true</active>
        <description>Once Mass Email Batch is completed, User can not change</description>
        <errorConditionFormula>IF(ISCHANGED(Batch_Status__c), ISPICKVAL(PRIORVALUE(Batch_Status__c),&quot;Completed&quot;), ISPICKVAL(Batch_Status__c, &quot;Completed&quot;))</errorConditionFormula>
        <errorMessage>Mass Email Batch is Complete. You cannot edit and reuse the record.</errorMessage>
    </validationRules>
