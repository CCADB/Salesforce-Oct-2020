    <fields>
        <fullName>User_Eligible_to_Send_Mass_Email__c</fullName>
        <externalId>false</externalId>
        <formula>IF (OR( 

AND(CONTAINS(From_In_Text__c, &quot;Mozilla&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Mozilla&quot;), 
CONTAINS($Profile.Name, &quot;Mozilla&quot;)), 

AND(CONTAINS(From_In_Text__c, &quot;Microsoft&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Microsoft&quot;), 
CONTAINS($Profile.Name, &quot;Microsoft&quot;)), 

AND(CONTAINS(From_In_Text__c, &quot;Google&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Google&quot;), 
CONTAINS($Profile.Name, &quot;Google&quot;)), 

AND(CONTAINS(From_In_Text__c, &quot;Current User&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Mozilla&quot;), 
CONTAINS($Profile.Name, &quot;Mozilla&quot;)), 

AND(CONTAINS(From_In_Text__c, &quot;Current User&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Microsoft&quot;), 
CONTAINS($Profile.Name, &quot;Microsoft&quot;)),

AND(CONTAINS(From_In_Text__c, &quot;Current User&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Google&quot;), 
CONTAINS($Profile.Name, &quot;Google&quot;)), 

AND(CONTAINS(From_In_Text__c, &quot;Mozilla&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Current User&quot;), 
CONTAINS($Profile.Name, &quot;Mozilla&quot;)), 

AND(CONTAINS(From_In_Text__c, &quot;Microsoft&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Current User&quot;), 
CONTAINS($Profile.Name, &quot;Microsoft&quot;)), 

AND(CONTAINS(From_In_Text__c, &quot;Google&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Current User&quot;), 
CONTAINS($Profile.Name, &quot;Google&quot;)), 

AND(CONTAINS(From_In_Text__c, &quot;CCADB Support&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Current User&quot;)),

AND(CONTAINS(From_In_Text__c, &quot;CCADB Support&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Mozilla&quot;), 
CONTAINS($Profile.Name, &quot;Mozilla&quot;)), 

AND(CONTAINS(From_In_Text__c, &quot;CCADB Support&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Microsoft&quot;), 
CONTAINS($Profile.Name, &quot;Microsoft&quot;)),

AND(CONTAINS(From_In_Text__c, &quot;CCADB Support&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Google&quot;), 
CONTAINS($Profile.Name, &quot;Google&quot;)),

AND(CONTAINS(From_In_Text__c, &quot;Current User&quot;), 
CONTAINS(Reply_To_In_Text__c, &quot;Current User&quot;))
), 
TRUE, FALSE)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Match profile of current/logged in user with &apos;From&apos; user and &apos;Reply To&apos; user (Used by Mass Email Batch pgm)</inlineHelpText>
        <label>DELETE User Eligible to Send Mass Email?</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
